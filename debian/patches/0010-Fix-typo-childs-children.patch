 src/Providers/music-tree.vala |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/src/Providers/music-tree.vala b/src/Providers/music-tree.vala
index d4e0897..be1e8bf 100644
--- a/src/Providers/music-tree.vala
+++ b/src/Providers/music-tree.vala
@@ -232,7 +232,7 @@ public class Gmpc.Provider.MusicTree : Gmpc.Plugin.Base, Gmpc.Plugin.MetaDataIfa
         string path = GLib.Path.build_filename(directory, base_path);
 
         log("Gmpc.Plugin.MusicTreeProvider", GLib.LogLevelFlags.LEVEL_DEBUG, 
-                "Got path: %s. Starting to itterate over childs", path);
+                "Got path: %s. Starting to itterate over children", path);
         /* Start async directory walking */
         var dir = File.new_for_path (path);
         try {
